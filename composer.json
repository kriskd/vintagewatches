{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "Datasources",
                "type": "cakephp-plugin",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/cakephp/datasources",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zeroasterisk/CakePHP-ArrayToXml-Lib",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/zeroasterisk/CakePHP-ArrayToXml-Lib",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "dev-master",
        "cakephp/cakephp": "2.10.*",
        "cakephp/debug_kit": "2.2.*",
        "mjohnson/uploader": "4.*",
        "Datasources": "dev-master",
        "chronon/stripe": "dev-master",
        "chronon/mobile_detect": "1.1.0",
        "zeroasterisk/CakePHP-ArrayToXml-Lib": "dev-master",
		"loadsys/cakephp_sitemap": "~1.0",
        "burzum/cakephp-html-purifier": "1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.* || 5.*",
        "burriko/cake-faker": "2.0.*@dev"
    },
    "config": {
        "vendor-dir": "Vendor"
    }
}
