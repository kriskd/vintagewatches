{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "DebugKit",
                "type": "cakephp-plugin",
                "version": "2.2.1",
                "source": {
                    "url": "https://github.com/cakephp/debug_kit",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "TwitterBootstrap",
                "type": "cakephp-plugin",
                "version": "2.1",
                "source": {
                    "url": "https://github.com/loadsys/twitter-bootstrap-helper",
                    "type": "git",
                    "reference": "origin/2.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "Stripe",
                "type": "cakephp-plugin",
                "version": "1.0.2",
                "source": {
                    "url": "https://github.com/chronon/CakePHP-StripeComponent-Plugin",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "stripe/stripe-php",
                "version": "1.8.3",
                "source": {
                    "url": "https://github.com/stripe/stripe-php",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twbs/bootstrap",
                "version": "3.0.0",
                "source": {
                    "url": "https://github.com/twbs/bootstrap",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kriskd/cakephp2.0-image-upload",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/kriskd/cakephp2.0-image-upload",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "font-awesome",
                "version": "3.2.1",
                "source": {
                    "url": "https://github.com/FortAwesome/Font-Awesome/",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "dev-master",
        "TwitterBootstrap": "2.1.*",
        "DebugKit": "2.2.1",
        "Stripe": "1.0.2",
        "stripe/stripe-php": "1.8.3",
        "twbs/bootstrap": "3.0.0",
        "kriskd/cakephp2.0-image-upload": "dev-master",
        "font-awesome": "v3.2.1"
    },
    "config": {
        "vendor-dir": "Vendor"
    },
    "extra": {
        "installer-paths": {
            "Plugin/Stripe/Vendor/Stripe": ["stripe/stripe-php"],
            "Controller/Component": ["kriskd/cakephp2.0-image-upload"]
        }
    }
}