{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "cakephp/debug_kit",
                "version": "2.0",
                "source": {
                    "url": "https://github.com/cakephp/debug_kit",
                    "type": "git",
                    "reference": "origin/2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "loadsys/twitter-bootstrap-helper",
                "version": "2.1",
                "source": {
                    "url": "https://github.com/loadsys/twitter-bootstrap-helper",
                    "type": "git",
                    "reference": "origin/2.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "chronon/stripe",
                "version": "master",
                "source": {
                    "url": "https://github.com/chronon/CakePHP-StripeComponent-Plugin",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "stripe/stripe-php",
                "version": "master",
                "source": {
                    "url": "https://github.com/stripe/stripe-php",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twbs/bootstrap",
                "version": "dev-3.0.0-wip",
                "source": {
                    "url": "https://github.com/twbs/bootstrap",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "malikov/cakephp2.0-image-upload",
                "version": "master",
                "source": {
                    "url": "git://github.com/malikov/cakephp2.0-image-upload.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "font-awesome",
                "version": "3.2.1",
                "source": {
                    "url": "https://github.com/FortAwesome/Font-Awesome/",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "loadsys/twitter-bootstrap-helper": "2.1.*",
        "cakephp/debug_kit": "2.0",
        "chronon/stripe": "*",
        "stripe/stripe-php": "*",
        "twbs/bootstrap": "dev-3.0.0-wip",
        "malikov/cakephp2.0-image-upload": "dev-master",
        "font-awesome": "v3.2.1"
    },
    "config": {
        "vendor-dir": "Vendor"
    },
    "extra": {
        "installer-paths": {
            "Plugin/{$name}": ["cakephp/debug_kit"],
            "Plugin/TwitterBootstrap": ["loadsys/twitter-bootstrap-helper"],
            "Plugin/Stripe": ["chronon/stripe"],
            "Plugin/Stripe/Vendor/Stripe": ["stripe/stripe-php"],
            "Controller/Component": ["malikov/cakephp2.0-image-upload"],
            "Vendor/twbs/bootstrap/less/font-awesome": ["font-awesome"]
        }
    }
}